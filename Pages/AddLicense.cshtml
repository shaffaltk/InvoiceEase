@page
@model BillingSystem.Pages.AddLicenseModel
<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>


<form method="post">
    <div class="form-group">
        <label asp-for="ScreenLicense.ScreenLicenseId" class="control-label"></label>
        <input asp-for="ScreenLicense.ScreenLicenseId" class="form-control" />
        <span asp-validation-for="ScreenLicense.ScreenLicenseId" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="ScreenLicense.LicenseKey" class="control-label"></label>
        <input asp-for="ScreenLicense.LicenseKey" class="form-control" />
        <span asp-validation-for="ScreenLicense.LicenseKey" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="ScreenLicense.StartDate" class="control-label"></label>
        <input asp-for="ScreenLicense.StartDate" class="form-control" type="date" />
        <span asp-validation-for="ScreenLicense.StartDate" class="text-danger"></span>
    </div>
 <div class="form-group">
        <label asp-for="ScreenLicense.LicenseTypeId" class="control-label"></label>
        <select asp-for="ScreenLicense.LicenseTypeId" class="form-control" asp-items="@(new SelectList(Model.Types, "LicenseTypeId", "Type"))"></select>
        <span asp-validation-for="ScreenLicense.LicenseTypeId" class="text-danger"></span>
    </div>

    <div class="form-group">
        <input type="submit" value="Add License" class="btn btn-primary" />

    </div>
</form>
